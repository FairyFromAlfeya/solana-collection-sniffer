{
  "name": "solana-collection-sniffer",
  "version": "0.0.1",
  "description": "solana-collection-sniffer",
  "author": "fsociety",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "lint": "eslint 'src/**/*.ts' --fix",
    "test": "jest",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main"
  },
  "dependencies": {
    "@fairyfromalfeya/fsociety-proto": "2.0.7",
    "@grpc/grpc-js": "1.6.2",
    "@grpc/proto-loader": "0.6.9",
    "@nestjs/cli": "8.2.4",
    "@nestjs/common": "8.4.3",
    "@nestjs/config": "2.0.0",
    "@nestjs/core": "8.4.3",
    "@nestjs/event-emitter": "1.1.0",
    "@nestjs/microservices": "8.4.3",
    "@nestjs/platform-express": "8.4.3",
    "@nestjs/typeorm": "8.0.3",
    "@solana/web3.js": "1.37.1",
    "joi": "17.6.0",
    "karneges-sbt": "0.0.13",
    "nestjs-redis": "git+https://github.com/skunight/nestjs-redis.git#master",
    "pg": "8.7.3",
    "redis": "4.0.6",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "typeorm": "0.2.45"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "16.2.1",
    "@nestjs/schematics": "8.0.9",
    "@nestjs/testing": "8.4.3",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.23",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "prettier": "2.6.2",
    "supertest": "6.2.2",
    "ts-jest": "27.1.4",
    "ts-loader": "9.2.8",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
